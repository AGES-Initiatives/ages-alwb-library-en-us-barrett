meMA.Ode8C1H.incipit = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.incipit
meMA.Ode8C1H.text = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.text

meMA.Ode8C11.ode = miscellanea_en_US_barrett.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode8C11.greekmelody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.greekname
meMA.Ode8C11.melody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.notmetered
meMA.Ode8C11.incipit = "Today let us bless Agabus…"
meMA.Ode8C11.text = "Today let us bless Agabus, Phlegon, Asyncritus, Rufus, illustrious Hermes, and Herodion, for they were divine Apostles of the Word, lights for the world, and teachers of the nations."

meMA.Ode8C12.ode = miscellanea_en_US_barrett.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode8C12.melody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.notmetered
meMA.Ode8C12.incipit = "The renowned Hermes was the…"
meMA.Ode8C12.text = "The renowned Hermes was the guide and luminary of all Dalmatia. He appeared there like a bright sun, he worked amazing miracles, and he enlightened hearts that had been blind."

meMA.Ode8C13.ode = miscellanea_en_US_barrett.misc.Ode8
meMA.Ode8C13.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode8C13.melody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.notmetered
meMA.Ode8C13.incipit = "O glorious Hermes, you carried…"
meMA.Ode8C13.text = "O glorious Hermes, you carried the Word as a bright lamp, you enlightened those who sat in the night of calamities, you performed healings and great signs, and you drew all the unbelievers to the faith."

meMA.Ode8C1Tr.ode = miscellanea_en_US_barrett.misc.Ode8
meMA.Ode8C1Tr.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode8C1Tr.melody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.notmetered
meMA.Ode8C1Tr.incipit = "As we honor the Trinity…"
meMA.Ode8C1Tr.text = "As we honor the Trinity, let us glorify the Father and faithfully worship the Son and the Holy Spirit, One Godhead in Three Persons, one in nature, whole and indivisible."

meMA.Ode8C1T.ode = miscellanea_en_US_barrett.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode8C1T.melody = he.h.m3_en_US_barrett.heHE.VavyloniaKaminos.notmetered
meMA.Ode8C1T.incipit = "O spotless Virgin, we are…"
meMA.Ode8C1T.text = "O spotless Virgin, we are your servants, and you are our protection and unfailing intercessor with your Son and God. O august Lady, save us from dangers and temptations, so that we may always glorify you with faith and zeal."

meMA.Ode9C1H.ode = miscellanea_en_US_barrett.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_en_US_barrett.misc.Mode3
meMA.Ode9C1H.greekmelody = he.h.m3_en_US_barrett.heHE.TinEnVatoKaiPyri.greekname
