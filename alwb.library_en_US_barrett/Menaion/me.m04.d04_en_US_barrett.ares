meMA.Ode1C1H.ode = miscellanea_en_US_barrett.misc.Ode1
meMA.Ode1C1H.mode = miscellanea_en_US_barrett.misc.Mode2
meMA.Ode1C1H.melody = he.h.m2_en_US_barrett.heHE.DefteLaoi.melody
meMA.Ode1C1H.incipit = he.h.m2_en_US_barrett.heHE.DefteLaoi.incipit
